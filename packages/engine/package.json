{
  "name": "stackmate",
  "version": "0.1.0",
  "author": {
    "name": "Fotis Alexandrou (@falexandrou)",
    "url": "https://www.falexandrou.com"
  },
  "bin": {
    "stackmate": "./bin/run"
  },
  "bugs": "https://github.com/stackmate-io/stackmate-ce/issues",
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.37.0",
    "@aws-sdk/types": "^3.40.0",
    "@cdktf/provider-aws": "^1.0.107",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "cdktf": "^0.4.1",
    "cdktf-cli": "^0.6.3",
    "chalk": "^4.1.1",
    "constructs": "^3.3.91",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "tslib": "^1",
    "validate.js": "^0.13.1",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@sinonjs/fake-timers": "6",
    "@types/chai": "^4.2.21",
    "@types/jsforce": "^1.9.35",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.2",
    "@types/react": "^17.0.26",
    "chai": "^4.3.4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5.2.0",
    "nyc": "^14",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=10.1.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://stackmate.io",
  "keywords": [
    "stackmate",
    "aws",
    "cloud",
    "terraform",
    "provisioning"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "stackmate",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/stackmate-io/stackmate-ce",
  "ts-node": {
    "transpileOnly": true,
    "files": true
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "oclif-dev": "node -r tsconfig-paths/register node_modules/.bin/oclif-dev",
    "cmd": "node -r tsconfig-paths/register ./bin/run",
    "prepack": "rm -rf lib && tsc --build tsconfig.build.json && yarn oclif-dev manifest && yarn oclif-dev readme",
    "test": "TS_NODE_PROJECT='./tsconfig.json' ./node_modules/.bin/mocha --opts tests/mocha.opts tests/**/*.test.ts",
    "test:ci": "npm run lint && npm run test",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
