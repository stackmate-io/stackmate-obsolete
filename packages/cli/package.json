{
  "name": "@stackmate/cli",
  "version": "0.1.0",
  "description": "Stackmate - launch and deploy cloud infrastructure using 1-line commands",
  "author": "Fotis Alexandrou @falexandrou",
  "bin": {
    "stackmate": "bin/run"
  },
  "homepage": "https://github.com/stackmate-io/cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stackmate-io/cli.git"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "@stackmate/engine": "^0.1.0",
    "ffi-napi": "^4.0.3",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4.3.0",
    "@types/faker": "^6.6.9",
    "@types/ffi-napi": "^4.0.5",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.179",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "chai": "^4.3.6",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.2",
    "eslint-plugin-import": "^2.25.4",
    "faker": "^6.6.6",
    "globby": "^11",
    "jest": "^27.5.1",
    "mocha": "^9",
    "oclif": "^2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2"
  },
  "oclif": {
    "bin": "stackmate",
    "dirname": "stackmate",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "make-bindings": "go build -o bindings/terraform.so -buildmode=c-shared bindings/terraform.go",
    "build": "rm -rf dist && tsc -b && npm run make-bindings",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"tests/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": {
    "url": "https://github.com/stackmate-io/cli/issues"
  },
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "directories": {
    "test": "tests"
  }
}
